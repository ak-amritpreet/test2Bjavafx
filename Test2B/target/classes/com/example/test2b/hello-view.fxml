<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.test2b.HelloController">


<padding>
<Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
</padding>
<Label fx:id="user_name" text="User name" />
<TextField fx:id="eBookID" text="BookID" />
<TextField fx:id="eTitle" text="Title" />
<TextField fx:id="eAuthor" text="Author" />
<TextField fx:id="eGenre" text="Genre" />


<Label fx:id="welcomeText" />
<Button onAction="#onHelloButtonClick" text="Click Here" />
<Button mnemonicParsing="false" onAction="#InsertData" text="Insert" />
<Button mnemonicParsing="false" onAction="#DeleteData" text="Delete" />
<Button mnemonicParsing="false" onAction="#UpdateData" text="Update" />
<Button mnemonicParsing="false" onAction="#LoadData" text="Load" />
<TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0">
<columns>
    <TableColumn fx:id="BookID" prefWidth="75.0" text="BookID" />
    <TableColumn fx:id="Title" prefWidth="75.0" text="Title" />
    <TableColumn fx:id="Author" prefWidth="75.0" text="Author" />
    <TableColumn fx:id="Genre" prefWidth="75.0" text="Genre" />

</columns>
</TableView>
</VBox>
